; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[WAT]

Test_Directory="res://tests"
Results_Directory="res://tests/results/WAT"
Minimize_Window_When_Running_Tests=false
TestStrategy={
"repeat": 1,
"strategy": "RunAll"
}
Tags=PoolStringArray(  )
Display=8
Window_Size=Vector2( 1280, 720 )

[application]

config/name="Zeffyr"
run/main_scene="res://scenes/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Zeffyr"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[audio]

default_bus_layout="res://assets/AudioLayout.tres"

[autoload]

Console="*res://addons/quentincaffeino-console/src/Console.tscn"
GameState="*res://src/Managers/GameStateManager.cs"
DebugManager="*res://src/Managers/DebugManager.cs"
TimeManager="*res://src/Managers/TimeManager.cs"
FadeManager="*res://src/Managers/FadeManager.cs"
DebuggingBag="*res://src/Managers/DebuggingBag.tscn"
MusicManager="*res://src/Managers/MusicManager.cs"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "quentincaffeino-console" )

[importer_defaults]

texture={
"compress/mode": 3,
"compress/normal_map": 2,
"detect_3d": false,
"flags/filter": false,
"flags/repeat": 1,
"flags/srgb": 1
}
scene={
"animation/import": false,
"materials/storage": 2,
"skins/use_named_skins": false
}

